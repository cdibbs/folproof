{
  "bugs": {
    "url": "http://github.com/cdibbs/folproof/issues"
  },
  "name": "folproof",
  "author": "",
  "version": "0.1.1",
  "engines": {
    "npm": ">=1.3.24"
  },
  "description": "A first-order logic proof verifier",
  "main": "./",
  "bin": "./lib/cli.js",
  "dependencies": {
    "es6-collections": "^0.5.5",
    "nomnom": "1.5.2",
    "path": "0.4.9",
    "reflect-metadata": "^0.1.2",
    "typescript-require": "^0.2.9-1"
  },
  "devDependencies": {
    "browserify": "latest",
    "chai": "^3.4.1",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-nodeunit": "^0.4.1",
    "grunt-jison": "^1.3.2",
    "grunt-mocha-test": "^0.12.7",
    "grunt-ts": "^5.3.0-beta.3",
    "jison": "^0.4.18",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "nodeunit": "^0.9.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0",
    "typescript": "^2.5.3"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "make test",
    "build": "npm-run-all clean tsc parsers webpack",
    "tsc": "tsc",
    "clean": "rimraf dist/",
    "webpack": "webpack",
    "parsers": "npm-run-all mkdir_parser jison-pl jison-fol",
    "mkdir_parser": "mkdirp dist/parsers/PL dist/parsers/FOL",
    "jison-pl": "jison src/parsers/PL/pl-parser.jison -m commonjs src/parsers/PL/pl-parser.jisonlex -o dist/parsers/PL/pl.js",
    "jison-fol": "jison src/parsers/FOL/fol-parser.jison -m commonjs src/parsers/FOL/fol-parser.jisonlex -o dist/parsers/FOL/fol.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cdibbs/folproof.git"
  },
  "keywords": [
    "fol",
    "folproof",
    "proof",
    "logic",
    "language",
    "first-order"
  ],
  "license": "MIT",
  "readmeFilename": "readme.md"
}
